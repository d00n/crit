-#=stylesheet_link_tag "prototip"
-#=stylesheet_link_tag "event_calendar"

.box
  %h2
    Your game calendar
  


.box
  = event_calendar.html_safe
  -if current_user
    %p
    = "Your time zone is #{current_user.time_zone}. #{link_to 'Change it here', edit_user_path(current_user)}".html_safe
    %p

    /.icon_row
    /  = link_to image_tag('/plugin_assets/community_engine/images/icons/ical.gif'), ical_user_url(current_user, :protocol => 'webcal://', :format=>'ics')
    /  = link_to image_tag("http://www.google.com/calendar/images/ext/gc_button1.gif"), "http://www.google.com/calendar/render?cid=#{ical_user_url(current_user, :format=>'ics')}", :target=>'_blank'
      
  
